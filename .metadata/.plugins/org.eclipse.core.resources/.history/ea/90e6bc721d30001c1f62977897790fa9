package designPatternStudy.patterns.builder;

public class BuilderMain {

	public static void main(String [] args) {
		//System.out.println("hello world");
		
		Text text = new Text.Builder()
					.setTitle("제목")
					.setContents("내용")
					.setItems("항목1", "항목2")
					.build();
		
		System.out.println(text);
		/**
		 * 실제 출력 
		 * 
		 * 타이틀 : 제목
		 * 내용 : 내용
		 * 항목 : 
		 *   - 항목1
		 *   - 항목2
		 * 
		 */
	}
}
